local TextBox1 = Instance.new("TextBox")
local TextLabel = Instance.new("TextLabel")
local TextBox2 = Instance.new("TextBox")
local TextLabel_2 = Instance.new("TextLabel")

--Properties:

PartSpawn.Name = "PartSpawn"
PartSpawn.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
PartSpawn.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Main.Name = "Main"
Main.Parent = PartSpawn
